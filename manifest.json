{
  "manifest_version": 2,
  "name": "TABsaver",
  "description": "save all the required tabs",
  "version": "1.0.0",
  "icons": {
    "128": "images/icon1.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}